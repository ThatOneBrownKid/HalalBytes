<div id="<%= dom_id restaurant %>">
  <p>
    <strong>Name:</strong>
    <%= restaurant.name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= restaurant.address %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= restaurant.phone %>
  </p>

  <p>
    <strong>Website:</strong>
    <%= restaurant.website %>
  </p>

  <p>
    <strong>Cuisine:</strong>
    <%= restaurant.cuisine %>
  </p>

  <p>
    <strong>Price range:</strong>
    <%= restaurant.price_range %>
  </p>

  <p>
    <strong>Overall rating:</strong>
    <%= restaurant.overall_rating %>
  </p>

</div>
